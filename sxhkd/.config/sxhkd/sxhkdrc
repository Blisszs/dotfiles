# text editor

super + shift + e
      emacs

# terminal

super + Return
      $TERMINAL -e tmux new -A -s st

# dmenu
super + d
      dmenu_run

# screenshot
Print
	maim ~/screenshots/screenshot-$(date '+%y%m%d-%H%M-%S').png

shift + Print
      maim -s ~/screenshots/screenshot-cropped$(date '+%y%m%d-%H%M-%S').png
